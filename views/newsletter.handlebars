<h2>Sign up for our newsletter to receive news and specials!</h2>
<form class="form-horizontal" role='form' 
		action="/process?form=newsletter" method="post">
	<input type="hidden" name="_csrf" value="{{csrf}}" />
	<div class="form-group">
		<label for="filedName" class="col-sm-2 control-label">Name</label>
		<div class="col-sm-4">
			<input type="text" class="form-control" id="fieldName" name="name" />
		</div>
	</div>
	<div class="form-group">
		<label for="filedEmai" class="col-sm-2 control-label">Emai</label>
		<div class="col-sm-4">
			<input type="text" class="form-control" id="fieldEmai" name="email" />
		</div>
	</div>
	<div class="form-group">
		<div class="col-sm-offset-2 col-sm-4">
			<button type="submit" class="btn btn-default">Register</button>
		</div>
	</div>
</form>
